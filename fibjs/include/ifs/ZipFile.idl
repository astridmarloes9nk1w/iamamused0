/*! @brief zip 文件访问对象 */
interface ZipFile : object
{
	/*! @brief 获取文件名列表 */
	List namelist() async;

	/*! @brief 获取文件信息列表 */
	List infolist() async;

	/*! @brief 获取文件信息 
	 @param member 指定要获取信息的文件名
	*/
	ZipInfo getinfo(String member) async;

	/*! @brief 返回从压缩文件读取的数据
     @param member 指定要读取的文件名
     @return 返回文件的所有数据
     */
	Buffer read(String member) async;

	/*! @brief 解压所有文件
     @return 包含所有文件数据及信息的列表
     */
	List readAll() async;

	/*! @brief 解压指定文件
     @param member 指定要解压的文件名
     @param path 指定要解压到的路径
     */
	extract(String member, String path) async;

	/*! @brief 解压指定文件到流
     @param member 指定要解压的文件名
     @param strm 指定要解压到的流
     */
	extract(String member, SeekableStream strm) async;

	/*! @brief 解压所有文件到指定路径
     @param path 指定要解压到的路径
     */
	extractAll(String path) async;

	/*! @brief 写入指定文件到压缩文件
     @param filename 指定要写入的文件
     */
	write(String filename) async;

	/*! @brief 写入指定文件到压缩文件
     @param data 指定要写入的文件数据
     */
	write(Buffer data) async;

	/*! @brief 写入指定文件到压缩文件
     @param data 指定要写入的文件数据
     */
	write(SeekableStream strm) async;

	/*! @brief 关闭打开的创建压缩文件 */
	close() async;
};
