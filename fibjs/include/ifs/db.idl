
/*! @brief 数据库访问模块
 *
 * 基础模块。可用于创建和操作数据库资源，引用方式：
 * \code
 * var db = require('db');
 * \endcode
 */
class db : module
{
	static DbConnection open(String connString);
	static DbConnection openMySQL(String connString);
	static DbConnection openMySQL(String host, Integer port, String username, String password, String dbName = "");
	static DbConnection openSQLite(String connString);
};
