
/*! @brief 包含 multipart 的一个条目数据 */
interface HttpUploadData : object
{
	/*! @brief 包含本条目数据的文件名 */
	readonly XString fileName;

	/*! @brief 包含本条目数据的类型 */
	readonly XString contentType;

	/*! @brief 包含本条目数据的传输编码类型 */
	readonly XString contentTransferEncoding;

	/*! @brief 包含本条目数据部分的流对象 */
	readonly SeekableStream body;
};
